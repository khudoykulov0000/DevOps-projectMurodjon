    /******************************  White Head  ********************************/    
    {
      "selector": [ "#tabs_top_panel" ],
      "background": "color(#E1E4E8 alpha(.8))"
    },
    {
      "selector": 
      [
        "#toolbar_panel",
        "#bookmarks_panel", 
        "#infobar_panel",
        "#tabs_top_panel[state=ntp][popup=suggest]",
        "#toolbar_panel[state=ntp][popup=suggest]",
        "#bookmarks_panel[state=ntp][popup=suggest]",
      ],
      "background": ["#fff"],
    },
    {
      "selector": "#smartbox_arrow",
      "foreground": "color (#EAECEF a(.7))",
    },
    {
      "selector": "#smartbox_arrow:is([state=ntp], :sly_zen)",
      "foreground": "#FFF",
    },
    {
      "selector": "#smartbox_arrow[state=ntp]:not(:yandex_search)[popup=suggest]",
      "foreground": "color (#EAECEF a(.7))",
    },
    {
      "selector": "#smartbox > #clicable_domain",
      "no_subpixel_rendering": true,
      "text_color": ["color(#000 a(.5))", "#09f", "#09f"],
      "background":["color(#000 a(.07))", "color(#000 a(.13))", "color(#000 a(.13))"],
      "radius": 3
    },
    {
      "selector": "#smartbox[theme=dark] > #clicable_domain",
      "no_subpixel_rendering": true,
      "text_color": ["color(#fff a(.5))", "#4cb0ff", "#4cb0ff"],
      "background":["#353741", "#292B33", "#23242A"],
    }, 
    {
      "selector": "#smartbox[theme=dark][state=protected] > #clicable_domain",
      "background":["#606060", "#545454", "#474747"],
    },
    {
      "selector": "#smartbox > #clicable_domain.conn_https.conn_ev",
      "text_color": ["#0DA909", "var(blue_hover)", "var(blue_hover)"],
    },


    /******************************  Tabs Panel  ********************************/    
    {
      "selector": [ "#tabs_top_panel" ],
      "caption_height": 8,
    },
    {
      "selector":
      [
        "#tabs_top_panel[window=fullscreen]",
        "#tabs_top_panel[window=maximized]",
      ],
      "caption_height": 0,
    },
    
    
    /****************************** Tab strip *******************************/
    {
      "selector": "#tab_strip > #custo_tab",
      "background": ["#0000", "#0000", "1a000000", "var(tab_background_color)"],
    },
    {
      "selector": "#tab_strip > #custo_tab:is([style=black]:not(:bottom_tabs), [style=dark])",
      "background": ["#0000", "#0000", "33ffffff", "var(tab_background_color)"],
    },
    {
      "selector": "#tab_strip > #custo_tab.active_look",
      "background": ["color(#ffffff a(.84))", "#ffffff", "1a000000", "var(tab_background_color)"],
    },
    {
      "selector": "#tab_strip > #custo_tab.active_look[style=dark]",
      "background": ["color(#fff a(.18))", "color(#fff a(.13))", "33ffffff", "var(tab_background_color)"],
    },     
 

